<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<title>Agregar Objeto</title>
    <link rel="stylesheet" href="/css/bootstrap.min.css">
</head>
<body class="container" style="margin-top: 50px;">

	<% if (messages.error) { %>
	<div class="alert alert-danger" role="alert"><%- messages.error %></div>
	<% } %>

    <div class="card"> 
    	<div class="card-header">
            Agregar Objeto
		</div>
        <div class="card-body">
            <form action="/objetos/add" method="post">
                <div class="form-group">
                    <label>Nombre:</label>
                    <input type="text" class="form-control" name="nombre" value="<%= nombre %>" autocomplete="off">
                </div>
                <div class="form-group">
                    <label>Descripc√≥n:</label>
                    <input type="text" class="form-control" name="descripcion" value="<%= descripcion %>" autocomplete="off">
                </div>
                <div class="form-group">
                    <input type="submit" class="btn btn-info" value="Agregar"/>
                </div>
            </form>
        </div>
    </div>

</body>
</html>
